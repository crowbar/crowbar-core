<% @heat_services.each do |s| %>
command[check_<%= s %>]=/usr/lib/nagios/plugins/check_procs -w 2:10 -c 1:100 -a "<%= s %>"
<% end unless @heat_services.nil? %>

<% @heat_ports.each do |name,dest| %>
command[check_port_<%= name %>]=/usr/lib/nagios/plugins/check_tcp -H <%= @heat_ip %> -p <%= dest %>
<% end unless @heat_ports.nil? %>

